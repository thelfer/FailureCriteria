* Mesh

STOT = LIRE 'MED' 'ct20.med';
SCT = STOT.'CT20JDP_66Q_ext_nosym' ELEM CU20;

PCT = SCT POIN PLAN (0. 0. 0.) (1. 0. 0.) (0. 1. 0.) 0.000001;
SCT = (CHAN SURFACE SCT) ELEM APPU STRICTEMENT PCT;
PCT = SCT POIN DROI (40.0 0. 0.) (40.0 1. 0.) 15.6; 
ELIM PCT 0.000001;
SCT = CHAN QUADRATIQUE SCT;

PGO1 = SCT POIN DROI (0. 11. 0.) (0. 11. 1.) 4.9;
SGO1 = SCT ELEM APPU LARG PGO1;

PGO2 = SCT POIN DROI (0. -11. 0.) (0. -11. 1.) 4.9;
SGO2 = SCT ELEM APPU LARG PGO2;

SGO = CHAN QUADRATIQUE (SGO1 ET SGO2);

SCT = SCT DIFF SGO;
ST = SCT ET SGO;

CMOD1 = ST POIN PROC (0. 1. 0.);
CMOD2 = ST POIN PROC (0. -1. 0.);

OPTI 'DIME' 2 'ELEM' 'QUA8';

PH = SGO1 POIN PROC (0. 11.);
PB = SGO2 POIN PROC (0. -11.);

TRAC ST;

OUBL PH;
OUBL PB;
OUBL CMOD1;
OUBL CMOD2;
OUBL SCT;
OUBL SGO;
OUBL SGO1;
OUBL SGO2;
OUBL PGO1;
OUBL PGO2;
OUBL PCT;
OUBL STOT;

OPTI SORT 'ct20_2d.med';
SORT MED ST;

FIN;
