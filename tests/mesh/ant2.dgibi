NN = 10;

* Options

OPTI DIME 2 ELEM QUA8;

* Mesh

OPTI DENS (1./NN);

L1 = (0. 0.) DROI (0. 0.25);
L2 = L1 PROJ (1. 0.) CERC (1.4 0.) (1. 0.);
S1 = L1 REGL L2;
S2 = (S1 COTE 2) TRAN (0. 2.75);
L3 = (S2 POIN PROC (1.4 3.)) DROI (1.4 3.);
L4 = L3 PROJ (0. -1.) CERC (1.4 0.) (1. 0.);
L5 = COTE 2 S2;
L6 = (L3 POIN PROC (1.4 3.)) DROI (L4 POIN PROC (1.4 0.4)) (NBEL L5);
ELIM (L4 ET L5) 0.000001;
S3 = DALL L3 L6 L4 L5 PLAN;
S4 = L6 TRAN (0.4 0.);

ST1 = S1 ET S2 ET S3 ET S4;
ST2 = ST1 SYME DROI (0. 0.) (1. 0.);
ST = ST1 ET ST2;
ELIM ST 0.000001;

TRAC ST;

OUBL L1;
OUBL L2;
OUBL S1;
OUBL S2;
OUBL L3;
OUBL L4;
OUBL L5;
OUBL L6
OUBL S3;
OUBL S4;
OUBL ST1;
OUBL ST2;

OPTI SORT (CHAI 'ant2' '_' NN '.med');
SORT MED ST;

FIN;

